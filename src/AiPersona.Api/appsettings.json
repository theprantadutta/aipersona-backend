{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Hangfire": "Warning"
    }
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning",
        "Hangfire": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Jwt": {
    "SecretKey": "",
    "Issuer": "AiPersona",
    "Audience": "AiPersonaApp",
    "AccessTokenExpireMinutes": 43200,
    "RefreshTokenExpireDays": 90
  },
  "Firebase": {
    "ProjectId": "",
    "CredentialsPath": "firebase-admin-sdk.json"
  },
  "Gemini": {
    "ApiKey": "",
    "Model": "gemini-2.0-flash-exp",
    "MaxTokens": 2048,
    "Temperature": 0.7
  },
  "Freeway": {
    "ApiUrl": "",
    "ApiKey": "",
    "Model": ""
  },
  "GooglePlay": {
    "ServiceAccountPath": "google-play-service-account.json",
    "PackageName": ""
  },
  "FileRunner": {
    "BaseUrl": "",
    "ApiKey": ""
  },
  "FreeTier": {
    "MessageLimitPerDay": 50,
    "PersonaLimit": 3,
    "HistoryRetentionDays": 7,
    "StorageLimitMb": 100
  },
  "AiDefaults": {
    "MaxTokens": 2048,
    "Temperature": 0.7,
    "MaxConversationHistory": 20
  },
  "Hangfire": {
    "SchemaName": "aipersona_hangfire",
    "Username": "",
    "Password": ""
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:5173"
    ]
  }
}
